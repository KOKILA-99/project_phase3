package com.demo.service;

import java.util.List;

import com.demo.model.Shoes;

public interface ShoeService {

	public Shoes addProduct(Shoes shoe);
	public Shoes updateProduct(Shoes shoe);
	public Shoes getProductById(int id);
	public void deleteProductById(int id);
	public List<Shoes> getAllProducts();
	public List<Shoes> getShoesByCategory(String category);
}
